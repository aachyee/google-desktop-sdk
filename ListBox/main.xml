<view width="200" height="120" showCaptionAlways="true" onopen="view_onOpen()">
  <script src="main.js" />  

  <label bold="true">&A_LISTBOX;</label>
  <div x="0" y="20" width="100%" height="100%">
    <listbox background="#FFFFFF"
             width="100%" height="100" x="0" y="0"
             itemWidth="100%" itemHeight="24" 
             itemSelectedColor="#99FF99" itemOverColor="#CCFFCC"
             name="listbox" onchange="lb_onchange()"
             autoscroll="true">
      <listitem name="item0"> 
        <label align="center" height="24" valign="middle">&LIST_ITEM_0;</label>
      </listitem>
      <listitem name="item1"> 
        <label align="center" height="24" valign="middle">&LIST_ITEM_1;</label>
      </listitem>
      <listitem name="item2" enabled="false"> 
        <label align="center" height="24" valign="middle" color="#AAAAAA">&LIST_ITEM_2;</label>
      </listitem>
      <listitem name="item3"> 
        <label align="center" height="24" valign="middle">&LIST_ITEM_3;</label>
      </listitem>
      <listitem name="item4"> 
        <img src="icon_small.png" width="24" height="24" x="2" y="0" />      
        <label align="center" height="24" valign="middle" x="26">&LIST_ITEM_4;</label>
      </listitem>
      <listitem name="item5"> 
        <label align="center" height="24" valign="middle">&LIST_ITEM_5;</label>
      </listitem>
    </listbox>    
  </div>
</view>
